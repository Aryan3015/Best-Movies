.spn-player-wrapper .hover-btn:hover { opacity: 0.7; }

.spn-player-wrapper .display--none { display: none !important; }

.spn-player-wrapper .display--flex { display: flex; }

.spn-player-wrapper .display--block { display: block; }

.spn-player-wrapper .visibility--hidden { opacity: 0; visibility: hidden; }

.spn-player-wrapper .visibility--visible { opacity: 1; visibility: visible; }

.spn-player-wrapper .visibility--ad-hidden, .spn-player-wrapper .visibility--ad-waiting, .spn-player-wrapper .visibility--skip-hidden { opacity: 0; visibility: hidden; }

.spn-player-wrapper .flex--center { justify-content: center; }

.spn-player-wrapper .flex--center, .spn-player-wrapper .flex--v-center { align-items: center; display: flex; }

.spn-player-wrapper .flex--h-center { display: flex; justify-content: center; }

.spn-player-wrapper .ripple { background: radial-gradient(circle, transparent 1%, rgb(255, 255, 255) 0px) 50% center / 0px rgb(255, 255, 255); transition: background 0s ease 0s; }

.spn-player-wrapper .ripple--active { background: radial-gradient(circle, transparent 1%, rgb(255, 255, 255) 0px) 50% center / 15000% rgba(255, 255, 255, 0.8); transition: background 0.8s ease 0s; }

.spn-player-wrapper .disable { cursor: unset; opacity: 0.6 !important; }

.spn-player-wrapper .disable:hover { opacity: 0.6 !important; }

.spn-player-wrapper .cursor--default { cursor: unset !important; }

.spn-player-wrapper .cursor--pointer { cursor: pointer; }

.spn-player-wrapper .scroll--disable { overflow-x: hidden; }

.spn-player-wrapper .preload-fonts { font-family: "SF Pro Display"; position: absolute; visibility: hidden; }

.spn-player-wrapper .font-roboto { font-family: Roboto; }

.spn-player-wrapper .font-400 { font-weight: 400; }

.spn-player-wrapper .font-500 { font-weight: 500; }

.spn-player-wrapper .font-600 { font-weight: 600; }

.spn-player-wrapper .font-700 { font-weight: 700; }

.spn-player-wrapper.extra-small-view .hover-btn:hover, .spn-player-wrapper.ipad-view .hover-btn:hover, .spn-player-wrapper.small-view .hover-btn:hover { opacity: 1; }
